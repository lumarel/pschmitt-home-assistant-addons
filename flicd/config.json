{
  "image": "lumarel/home-assistant-addon-{arch}-flicd",
  "name": "flicd",
  "arch": [
    "armhf",
    "aarch64",
    "i386",
    "amd64"
  ],
  "version": "2.0.3",
  "slug": "flicd",
  "description": "React to flic button presses",
  "startup": "services",
  "boot": "auto",
  "ports": {
    "5551/tcp": 5551
  },
  "host_network": "true",
  "privileged": [
    "NET_ADMIN"
  ],
  "options": {
    "hci_dev": "hci0"
  },
  "schema": {
    "hci_dev": "str"
  }
}
